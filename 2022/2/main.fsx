open System

type Res =
    | Win
    | Lose
    | Draw
    | Nil

type RPS =
    | R of int
    | P of int
    | S of int
    | Invalid

let rpsRepr str =
    match str with
    | "A"
    | "X" -> R 1
    | "B"
    | "Y" -> P 2
    | "C"
    | "Z" -> S 3
    | _ -> Invalid

let resRepr str =
    match str with
    | "X" -> Lose
    | "Y" -> Draw
    | "Z" -> Win
    | _ -> Nil

let parsed (input: string) =
    input.Split("\n")
    |> Array.map (fun str -> str.Trim().Split(" "))
    |> Array.map (fun arr -> arr[0], arr[1])


let ans1 =
    (parsed input)
    |> Array.map (fun (a, b) -> rpsRepr a, rpsRepr b)
    |> Array.map (fun (opp, player) ->
        match opp, player with
        | S _, R x
        | P _, S x
        | R _, P x -> 6 + x
        | S x, S _
        | P x, P _
        | R x, R _ -> 3 + x
        | _, R x
        | _, P x
        | _, S x -> x
        | _ -> 0)
    |> Array.sum

printfn $"{ans1}"

let ans2 =
    (parsed input)
    |> Array.map (fun (a, b) -> rpsRepr a, resRepr b)
    |> Array.map (fun (opp, res) ->
        match opp, res with
        | R x, Draw
        | P x, Draw
        | S x, Draw -> 3 + x
        | R _, Win -> 6 + 2
        | P _, Win -> 6 + 3
        | S _, Win -> 6 + 1
        | R _, Lose -> 3
        | P _, Lose -> 1
        | S _, Lose -> 2
        | _ -> 0)
    |> Array.sum

printfn $"{ans2}"
